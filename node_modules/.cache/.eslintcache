})\n        } else {\n            setSearchParams({\n            _limit: 12,\n            _page: page,\n    });\n    }\n    }, []);\n\n    useEffect(() => {\n    getSummer();\n  }, [searchParams]);\n\n    useEffect(() => {\n        if (screenWidth < 321) {\n            setSearchParams({\n            _limit: 4,\n            _page: page,\n            })\n            } else {\n            setSearchParams({\n            _limit: 12,\n            _page: page,\n    });\n    }\n    }, [page]);\n    \n\n    return (\n        <div style={{backgroundColor: \"#ECECEC\"}}>\n            <div className='container'>\n                <h3 className='summer-title'>Коллекция Лето 2022</h3>\n                <div style={{display: \"flex\", flexWrap: \"wrap\"}}>\n                    {summer.map((item) => (\n                        <SummerCard item={item} key={item.id}/>\n                    ))}\n            </div>\n                <div className='summer-pagination'>\n                    <Pagination\n                    variant=\"outlined\" classes={{ ul: classes.ul }} \n                    count={+summerCount}\n                    page={+page}\n                    onChange={(event, pageVal) => setPage(pageVal)}\n                    />\n                </div>\n            </div>\n            <New/>\n        </div>\n    );\n};\n\nexport default Summer;","/Users/makhabatabdisattarova/Desktop/ZEON IT HUB/main-project/src/context/Brandnew.js",[],[],"/Users/makhabatabdisattarova/Desktop/ZEON IT HUB/main-project/src/components/New/New.jsx",["285"],[],"import React, { useContext, useEffect } from 'react';\nimport { newContext } from '../../context/Brandnew';\nimport NewCard from './NewCard';\nimport TableCell from '@mui/material/TableCell';\nimport TableContainer from '@mui/material/TableContainer';\nimport TableHead from '@mui/material/TableHead';\nimport TableRow from '@mui/material/TableRow';\nimport Table from '@mui/material/Table';\nimport Paper from '@mui/material/Paper';\nimport \"./NewCar